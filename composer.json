{
  "name": "iss-module/user-module",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "google/apiclient": "^2.11",
    "guzzlehttp/guzzle": "^7.4",
    "laminas-api-tools/api-tools-oauth2": "^1.7",
    "laminas/laminas-authentication": "^2.11",
    "laminas/laminas-db": "^2.15",
    "laminas/laminas-hydrator": "^4.3",
    "laminas/laminas-mvc": "^3.1",
    "laminas/laminas-mvc-form": "^2.0",
    "laminas/laminas-mvc-plugin-flashmessenger": "^1.8",
    "laminas/laminas-permissions-rbac": "^3.2",
    "laminas/laminas-session": "^2.13",
    "laminas/laminas-uri": "^2.9",
    "nesbot/carbon": "^2.59"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "laminas/laminas-test": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "UserModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UserModuleTest\\": "test/"
    }
  },
  "extra": {
    "laminas": {
      "component": "UserModule"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --no-coverage",
    "cover": "vendor/bin/phpunit --coverage-html .coverage"
  },
  "config": {
    "sort-packages": true
  }
}
